{
    "compilerOptions": {
        "module": "commonjs",
        "allowJs": true,
        "outDir": "",
        "target": "es6",
        "noImplicitAny": false,
        "sourceMap": false,
        "noUnusedLocals": true,
        "allowUnreachableCode": false,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "lib": [
            "es2015"
        ]
    },
    "include": [
        "index.ts", "testImage.ts"
    ]
}
